  "publishConfig": {
    "tag": "alpha"
  },
  "private": false,
  "sideEffects": false,
  "description": "Reatom for react-v1",
  "source": "src/index.ts",
  "exports": {
    "require": "./build/index.js",
    "default": "./build/index.module.js"
